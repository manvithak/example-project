{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": [
    "react-native"
  ],
  "env": {
    "es6": true,
    "jest": true
  },
  "ecmaFeatures": {
    "jsx": true
  },
  "rules": {
    "react/jsx-filename-extension": [
      "error",
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": 0,
    "import/extensions": 0,
    "global-require": "off",
    "jsx-quotes": [
      "error",
      "prefer-double"
    ],
    "semi": [
      2,
      "never"
    ],
    "comma-dangle": [
      2,
      "never"
    ],
    "object-curly-spacing": [
      "error",
      "always"
    ],
    "no-shadow": 0,
    "prefer-destructuring": [
      "error",
      {
        "array": false,
        "object": true
      }
    ],
    "react/forbid-prop-types": 0,
    "react/no-unescaped-entities": 0,
    "no-confusing-arrow": 0,
    "import/prefer-default-export": 0
  },
  "globals": {
    "navigator": true,
    "alert": true,
    "fetch": true
  }
}
